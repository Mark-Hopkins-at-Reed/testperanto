{
    "distributions": [
        {"name": "vb", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "vb.1", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "vb.2", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "vb.3", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "vb.1.$y0", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "vb.2.$y0", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "vb.3.$y0", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "nn", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "nn.loc", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "nn.time", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "nn.arg0", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "nn.arg1", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "nn.arg2", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "nn.arg0.$y0", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "nn.arg1.$y0", "type": "pyor", "strength": 100, "discount": 0.5},
        {"name": "nn.arg2.$y0", "type": "pyor", "strength": 100, "discount": 0.5}
    ],
    "rules": [
        {"rule": "$qstart -> $qevent.0"},
        {"rule": "$qevent.$y1 -> (EVENT $qvb.$z1 $qargs.$z1 $qmods)",  "base_weight": 0.6, "zdists": ["vb.2.$y1"]},
        {"rule": "$qevent.$y1 -> (EVENT $qvb.$z1 $qargs.$z1 $qmods)",  "base_weight": 0.6, "zdists": ["vb.2.$y1"]},
        {"rule": "$qrcevent -> (EVENT $qvb.$z1 $qargsrc.$z1 $qmods)",  "base_weight": 0.6, "zdists": ["vb.2"]},
        {"rule": "$qccomp.$y1 -> (EVENT $qvb.$z1 $qargsccomp.$z1 $qmods)",  "base_weight": 0.6, "zdists": ["vb.2.$y1"]},
        {"rule": "$qentity.$y1 -> (ENTITY $qnn.$z1 $qentitymods)", "base_weight": 0.1, "zdists": ["nn.$y1"]},
        {"rule": "$qentity.$y1.$y2 -> (ENTITY $qnn.$z1 $qentitymods)", "base_weight": 0.2, "zdists": ["nn.$y1.$y2"]},
        {"rule": "$qvb.$y1 -> (inst vb.$y1)"},
        {"rule": "$qnn.$y1 -> (inst nn.$y1)"},
        {"rule": "$qargs.$y1 -> (args $qarg0.$y1 $qarg1.$y1)", "base_weight": 0.8},
        {"rule": "$qargs.$y1 -> (args $qarg1.$y1)", "base_weight": 0.2},
        {"rule": "$qargsccomp.$y1 -> (args $qarg1ccomp.$y1)"},
        {"rule": "$qargsrc.$y1 -> (args $qarg1.$y1)"},
        {"rule": "$qarg0.$y1 -> (arg0 $qentity.arg0.$y1)", "base_weight": 0.9},
        {"rule": "$qarg1.$y1 -> (arg1 $qentity.arg1.$y1)", "base_weight": 0.8},
        {"rule": "$qarg1.$y1 -> (arg1 $qccomp.$y1)", "base_weight": 0.2},
        {"rule": "$qarg1ccomp.$y1 -> (arg1 $qentity.arg0.$y1)"},
        {"rule": "$qentitymods -> -null-", "base_weight": 0.2},
        {"rule": "$qentitymods -> (mods $qentitymod)", "discount_factor": 0.5},
        {"rule": "$qentitymod -> (arg0-of $qrcevent)"},
        {"rule": "$qmods -> -null-"},
        {"rule": "$qmods -> (mods $qlocation)", "base_weight": 0.0},
        {"rule": "$qmods -> (mods $qtime)", "base_weight": 0.0},
        {"rule": "$qmods -> (mods $qsubclause)", "base_weight": 0.8, "discount_factor": 0.5},
        {"rule": "$qsubclause -> (arg1-of $qcause)"},
        {"rule": "$qsubclause -> (condition $qevent.0)", "base_weight": 0.0},
        {"rule": "$qcause -> (EVENT (inst cause.0) (args (arg0 $qevent.0)) -null-)"},
        {"rule": "$qlocation -> (location $qentity.loc)"},
        {"rule": "$qtime -> (time $qentity.time)"}
    ]
}
